# MongoDB connection string
MONGODB_URI=mongodb://localhost:27017/playliner

# Server port
PORT=3000

# Playliner API Bearer Token
BEARER_TOKEN=your_bearer_token_here

# Cron schedule (default: every 10 minutes)
CRON_SCHEDULE=*/10 * * * *

# Batch size for processing (how many IDs to process per cron run)
BATCH_SIZE=10

# Delay between API calls (in milliseconds)
API_DELAY=1000
